#include <iostream>
#include <iomanip>
#include <time.h>
using namespace std;
int main()
{
	double x, y, R;
	srand((unsigned)time(NULL));
	for (int i = 0; i < 10; i++)
	{
		cout << "R = "; cin >> R;
		cout << "x = "; cin >> x;
		cout << "y = "; cin >> y;
		if (
			(y >= 0 && y <= R && x >= -R && x <= R
				&& (pow(x, 2) + pow(y, 2) - R >= 0)) ||
			(y >= -R && y <= 0 && x >= -R && x <= 0 && y <= R * sqrt(2))
			)
			cout << "yes" << endl;
		else
			cout << "no" << endl;
	}
	cout << endl << fixed;

	for (int i = 0; i < 10; i++)
	{
		x = 6. * rand() / RAND_MAX - R;
		y = 6. * rand() / RAND_MAX - R;
		if (
			(y >= 0 && y <= R && x >= -R && x <= R
				&& (pow(x, 2) + pow(y, 2) - R >=0)) ||
			(y >= -R && y <= 0 && x >= -R && x <= 0 && y <= R * sqrt(2))
			)
			cout << setw(8) << setprecision(4) << x << " "
			<< setw(8) << setprecision(4) << y << " " << "yes" << endl;
		else
			cout << setw(8) << setprecision(4) << x << " "
			<< setw(8) << setprecision(4) << y << " " << "no" << endl;
	}
	return 0;
}
